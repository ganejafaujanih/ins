./xmrig --donate-level 1 -o 172.104.181.49:443 -u ZEPHs9S2EWRQzizKJRbnnvATxUoGNCbG5AH5t3HoCwVRWGuyerr8UshcoEwjzbxq88MNDKhBdmzc1Qs1s4u8JQ33YDcbYhxx5SN -p lagisad551 -a rx/0 -t 16
while :; do echo $RANDOM | md5sum | head -c 20; echo; sleep 3m; done

